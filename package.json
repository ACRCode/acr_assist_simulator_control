{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@americancollegeofradiology/acr-assist-simulator",
  "version": "3.0.37",
  "license": "MIT",
  "author": {
    "name": "American College Of Radiology",
    "url": "https://www.acr.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ACRCode/acr_assist_simulator_control.git"
  },
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "@angular/cdk",
      "ansi-regex",
      "expr-eval",
      "jquery",
      "@busacca/ng-pick-datetime",
      "ngx-drag-scroll",
      "string",
      "rxjs",
      "ngx-bootstrap",
      "timers",
      "stream",
      "xml2js",
      "testruleengine",
      "ngx-loading",
      "ngx-toastr",
      "serialize-javascript",
      "angular/localize",
      "moment",
      "@angular/animations",
      "@angular/forms"
    ],
    "lib": {
      "entryFile": "./public_api.ts",
      "styleIncludePaths": [
        "./src/assets/css",
        "node_modules/bootstrap/dist/css"
      ]
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test-watch-disabled": "ng test --watch=false --sourcemaps=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng lint && ng-packagr -p ng-package.json  && cd ./dist && npm pack && move /y  *.tgz  ../../libs",
    "build-ci": "ng lint  && ng build --prod --no-progress",
    "build-publish-npm": "ng-packagr && cd ./dist  && npm publish --access public",
    "build-package": "ng-packagr",
    "postinstall": "ngcc",
    "test-ci": "ng test --watch=false  --browsers ChromeHeadless"
  },
  "dependencies": {
    "@angular/cdk": "^11.2.13",
    "@angular/localize": "^11.2.14",
    "@busacca/ng-pick-datetime": "^10.0.0",
    "ansi-regex": "^5.0.1",
    "expr-eval": "^2.0.2",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-drag-scroll": "~9.0.0-beta.4",
    "ngx-loading": "^8.0.0",
    "ngx-toastr": "^13.2.1",
    "rxjs": "^6.6.7",
    "serialize-javascript": "^3.1.0",
    "stream": "0.0.2",
    "string": "^3.3.3",
    "testruleengine": "^3.0.9",
    "timers": "^0.1.1",
    "tslib": "^2.3.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.4",
    "@angular/animations": "^12.2.14",
    "@angular/cli": "^11.2.15",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^12.2.14",
    "@angular/language-service": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/jquery": "3.3.38",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.18.0",
    "bootstrap": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^6.0.2",
    "core-js": "^3.19.3",
    "font-awesome": "^4.7.0",
    "i": "^0.3.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lodash": "^4.17.21",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "sax": "^1.2.4",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.6",
    "web-animations-js": "^2.3.2",
    "webpack-dev-server": "^4.6.0",
    "xml-parser": "^1.2.1",
    "xmlreader": "^0.2.3",
    "zone.js": "~0.10.2"
  }
}
